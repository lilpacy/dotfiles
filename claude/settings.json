{
  "env": {
    "CLAUDE_CODE_USE_BEDROCK": "1",
    "ANTHROPIC_MODEL": "opus",
    "ANTHROPIC_DEFAULT_OPUS_MODEL": "global.anthropic.claude-opus-4-6-v1",
    "ANTHROPIC_DEFAULT_SONNET_MODEL": "jp.anthropic.claude-sonnet-4-5-20250929-v1:0",
    "CLAUDE_CODE_MAX_OUTPUT_TOKENS": "16384",
    "MAX_THINKING_TOKENS": "1024",
    "MCP_TOOL_TIMEOUT": "900000"
  },
  "permissions": {
    "allow": [
      "Read(~/**)",
      "WebSearch",
      "WebFetch",
      "Fetch",
      "Grep",
      "Glob",
      "Bash(ls:*)",
      "Bash(ls -l:*)",
      "Bash(ls -a:*)",
      "Bash(ls -la:*)",
      "Bash(cat:*)",
      "Bash(pwd:*)",
      "Bash(cd:*)",
      "Bash(find:*)",
      "Bash(grep:*)",
      "Bash(grep -n:*)",
      "Bash(ls:*)",
      "Bash(head:*)",
      "Bash(echo:*)",
      "Bash(npx tsc:*)",
      "Bash(git show:*)",
      "mcp__ais__gpt5_reason_browse",
      "mcp__codex__codex_exec",
      "WebFetch(domain:registry.npmjs.org)",
      "WebFetch(domain:eu-central-1-1.aws.cloud2.influxdata.com)",
      "WebFetch(domain:formulae.brew.sh)",
      "WebFetch(domain:pypi.org)",
      "WebFetch(domain:files.pythonhosted.org)"
    ],
    "deny": [
      "Read(./.env)",
      "Read(./.env.*)",
      "Read(./**/.env)",
      "Read(./**/.env.*)",
      "Read(./secrets/**)",
      "Read(~/.ssh/**)",
      "Read(~/.aws/**)",
      "Read(./config/credentials.json)",
      "Read(./build/**)",
      "Edit(./.env)",
      "Edit(./.env.*)",
      "Edit(./**/.env)",
      "Edit(./**/.env.*)",
      "Edit(./secrets/**)",
      "Edit(~/.ssh/**)",
      "Edit(~/.aws/**)",
      "Edit(./config/credentials.json)",
      "Edit(./build/**)",
      "Bash(sudo:*)",
      "Bash(rm -rf:*)",
      "Bash(git push:*)",
      "Bash(git reset:*)",
      "Bash(git rebase:*)"
    ]
  },
  "model": "opus",
  "hooks": {
    "Notification": [
      {
        "matcher": "permission_prompt|idle_prompt",
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Blow.aiff",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "tmux set -p -t $TMUX_PANE @claude_waiting 1",
            "timeout": 5
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "afplay /System/Library/Sounds/Blow.aiff && tmux set -p -t $TMUX_PANE @claude_waiting 1",
            "timeout": 5
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "tmux set -p -t $TMUX_PANE @claude_waiting 0",
            "timeout": 5
          }
        ]
      }
    ],
    "UserPromptSubmit": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "tmux set -p -t $TMUX_PANE @claude_waiting 0",
            "timeout": 5
          }
        ]
      }
    ],
    "SessionEnd": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "tmux set -p -t $TMUX_PANE @claude_waiting 0",
            "timeout": 5
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "bash -c '\"/Users/lilpacy/.asdf/shims/bun\" \"$(ls -td ~/.claude/plugins/cache/claude-hud/claude-hud/*/ 2>/dev/null | head -1)src/index.ts\"'"
  },
  "enabledPlugins": {
    "clangd-lsp@claude-plugins-official": true,
    "claude-hud@claude-hud": true
  },
  "sandbox": {
    "enabled": false,
    "autoAllowBashIfSandboxed": true,
    "network": {
      "allowedDomains": [
        "github.com",
        "*.npmjs.org",
        "registry.npmjs.org"
      ]
    },
    "excludedCommands": [
      "docker",
      "docker-compose"
    ]
  },
